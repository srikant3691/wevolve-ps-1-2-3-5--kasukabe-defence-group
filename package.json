{
  "name": "wevolve-monorepo",
  "version": "1.0.0",
  "description": "AI-Powered Career Acceleration Ecosystem",
  "private": true,
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn app.main:app --reload",
    "install:all": "npm install --prefix frontend && pip install -r backend/requirements.txt",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
